{
    "services": {
        "backends": {
            "~/path/to/your/code": {
                "commands": [
                    "source env/bin/activate",
                    "make dev"
                ]
            }
        },
        "graphqls": {},
        "frontends": {}
    },
    "defaultCommands": {
        "backends": [
            "make dev"
        ],
        "graphqls": [
            "yarn start"
        ],
        "frontends": [
            "yarn start"
        ]
    },
    "selectedByDefault": [
        "~/path/to/your/code"
    ]
}
